{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "^1.9.215",
    "gatsby-link": "^1.6.37",
    "gatsby-plugin-catch-links": "^1.0.17",
    "gatsby-plugin-react-helmet": "^2.0.5",
    "gatsby-source-filesystem": "^1.5.24",
    "gatsby-transformer-remark": "^1.7.33",
    "netlify-identity-widget": "file:netlify-identity-widget",
    "react-helmet": "^5.2.0",
    "react-media": "^1.8.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "postinstall": "cd netlify-identity-widget && npm install",
    "prebuild": "rimraf public && cd netlify-identity-widget && npm run build",
    "build": "gatsby build --prefix-paths",
    "postbuild": "npm run postbuild:docs && npm run postbuild:outside && npm run postbuild:netlify",
    "postbuild:docs": "rimraf pubtmp && mv public pubtmp && mkdir public && mv pubtmp public/docs",
    "postbuild:outside": "cp -R outside/* public",
    "postbuild:netlify": "cp netlify-identity-widget/build/netlify-identity-widget.js public",
    "develop": "gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2"
  }
}
